/**
 * @param {number[]} digits
 * @return {number[]}
 */
var plusOne = function(digits) {
    let length = digits.length;
    if(digits[length-1] != 9){
        digits[length-1] += 1;
    }
    else{
        if(digits[0] != 9){
            let i=length-1;
            while(digits[i] === 9){
               digits[i] = 0;
                i--;
            }
            digits[i] += 1;
        }
        else{
            let check = false;
            for(let i  = 0; i < length; i++){
              if(digits[i] != 9 ){
                check = true;
              }
            }
            if(check){
                let i = length-1;
                while(digits[i] === 9){
                digits[i] = 0;
                    i--;
                }
                digits[i] +=1;
            }else
                for(let i = length; i >= 0; i-- ){
                    if(i===0){
                        digits[i] = 1;
                    }
                    else{
                        digits[i] = 0;
                    }
                }
        }
    }
    return digits;
};
