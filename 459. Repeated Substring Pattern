/**
 * @param {string} s
 * @return {boolean}
 */
var repeatedSubstringPattern = function(s) {
    let n = s.length;
    for(let i = 0; i < n; i++){
       if(n % i == 0){
         let p = s.substr(0,i);
         let k = n/i;
         let t = p.repeat(k);
         if(t===s)
         return true
       }
    }
    return false;
};
